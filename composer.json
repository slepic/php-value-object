{
    "name": "slepic/value-object",
    "description": "Simple value objects and enums",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "slepic",
            "email": "gslepic@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.4",
        "ext-json": "*"
    },
    "require-dev": {
        "ext-mbstring": "*",
        "vimeo/psalm": "^3.11",
        "phpunit/phpunit": "^9.1",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "suggest": {
        "ext-mbstring": "Install if you want to use the multi-byte string length value objects."
    },
    "autoload": {
        "psr-4": {
            "Slepic\\ValueObject\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Slepic\\Tests\\ValueObject\\": "tests"
        }
    }
}
